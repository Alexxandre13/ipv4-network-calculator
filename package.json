    "ip",
    "ipv4",
    "network",
    "subnet",
    "Subnetwork"
  ],
  "author": "Alexandre Dos Reis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexandre-dos-reis/ipv4-calc/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/alexandre-dos-reis/ipv4-calc#readme",
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "jest": "^28.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  }
}
