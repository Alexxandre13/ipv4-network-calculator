{
  "compilerOptions": {
    "target": "es5",
    "lib": ["ES2017", "DOM"],
    "module": "CommonJS",
    "declaration": true,
    "outDir": "./dist",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }, 
  "include": ["src/**/*"],
  "exclude": ["./src/**/*.test.ts"]
}